<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrdr">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MrDR">



        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity2"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginActivity3"
            android:exported="true"
            android:parentActivityName=".SignUpActivity2"
            android:windowSoftInputMode="stateVisible" />
        <activity
            android:name=".HomepageActivity4"
            android:exported="true"
            android:parentActivityName=".LoginActivity3" />

    </application>

</manifest>